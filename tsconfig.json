{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "module": "esnext",
    "lib": ["es2017", "es2019","dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src"]
}
